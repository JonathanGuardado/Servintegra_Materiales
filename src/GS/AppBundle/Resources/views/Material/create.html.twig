{% extends '::frontend.html.twig' %}

{% block title %} Principal Servintegra {% endblock %}

{% block id 'portada' %}

{% block main %}
 
<section>
    <div class="naranja space">&nbsp;</div>
    <div class="space-line">&nbsp;</div>
    <div class="naranja space-line">&nbsp;</div>
</section>
<section>
    <article class="w100 gris_claro-textura space">&nbsp;</article>
    <article class="w100 gris_claro-textura space">&nbsp;</article>  
    <article class="w100 gris_claro-textura space"> &nbsp;</article>
    <article class="w100 gris_claro-textura space"> &nbsp;</article>
    <article class="w100 gris_claro-textura space border-colorgray">&nbsp;</article>  
</section>

<div class="container">

    <section> 
        <div class="L w50">
            <img src="{{ asset('bundles/app/images/line-shadow.png') }}" class="line_shadowa"> 
        </div>
            <br/>
        <h1 class="L L3">Nuevo Material</h1>
        <h3 class="L L15 T3">C&oacute;digo  Material: <font style="color:red">{{id}}</font></h3>
        
            {% for flashMessage in app.session.flashbag.get('info') %}
                <div class="alert alert-info alert-dismissible" role="alert">
                    <button type="button" class="close" data-dismiss="alert">
                        <span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
                    </button>
                    <strong>Aviso, </strong> {{ flashMessage }}
                </div>
            {% endfor %}
    {{ form(formulario, {'attr': {'class': ' formulario T8'}}) }}
        

            <div class="L w100">
                <img src="{{ asset('bundles/app/images/line-shadow.png') }}" class="line_shadowa"> 
            </div>
        </section>

    </div> <!-- /container -->



{% endblock %}