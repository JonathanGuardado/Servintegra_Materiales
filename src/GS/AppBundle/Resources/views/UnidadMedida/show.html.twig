    <table class="record_properties T2">
        <tbody>
           
            <tr>
                <th>Unidad de medida: </th>
                <td> {{ entity.nombreUnidad }}</td>
            </tr>
            <tr>
                <th>Descripcion: </th>
                <td> {{ entity.descripcionUnidad }}</td>
            </tr>
        </tbody>
    </table>

<hr>
   <div class="B5">
